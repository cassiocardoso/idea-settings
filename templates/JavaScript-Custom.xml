<templateSet group="JavaScript-Custom">
  <template name="jqr" value="$(function () {&#10;    console.log('ready');&#10;});" description="jQuery Ready" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="log" value="console.log($END$);" description="Console.log" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fun" value="function $NAME$ () {&#10;&#9;$END$&#10;}" description="function template" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="jsMethodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fin" value="function (d) { return d;$END$ }" description="function inline" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fan" value="function ($VAR$) {&#10;&#9;$END$&#10;}" description="anonymous function" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="$t" value="var $this = $(this);&#10;" description="$this = $(this)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="iife" value="(function () {&#10;&#9;$END$&#10;})();" description="Immediately-Invoked Function Expression" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="st" value="setTimeout(function() {&#10;&#9;$END$&#10;}, $TIME$);" description="setTimout" toReformat="true" toShortenFQNames="true">
    <variable name="TIME" expression="" defaultValue="&quot;1000&quot;" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pf" value="$PROP$: function ($VAR$) {&#10;&#9;$END$&#10;}" description="property function" toReformat="true" toShortenFQNames="true">
    <variable name="PROP" expression="jsMethodName()" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="_e" value="_.each($COL$, function($VAR$) {&#10;&#9;$END$&#10;});" description="Underscore Each" toReformat="true" toShortenFQNames="true">
    <variable name="COL" expression="jsArrayVariable()" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="si" value="setInterval(function() {&#10;&#9;$END$&#10;}, $TIME$);" description="SetInterval" toReformat="true" toShortenFQNames="true">
    <variable name="TIME" expression="" defaultValue="&quot;1000&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="us" value="'use strict';&#10;$END$" description="Inserts 'use strict' statement" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="jsh-l" value="// jshint ignore:line" description="jshint ignore line" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="comm" value="/*&#10;|-----------------------------------------------------------------------------&#10;| $END$&#10;|-----------------------------------------------------------------------------&#10;*/" description="Comment Block" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="itin" value="for (var $VAR$ in $ARRAY$) {&#10;&#9;if ($ARRAY$.hasOwnProperty($VAR$)) {&#10;  &#9;&#9;$END$&#10;  &#9;}&#10;}" description="Iterate (for..in)" toReformat="true" toShortenFQNames="true">
    <variable name="ARRAY" expression="jsArrayVariable()" defaultValue="&quot;array &quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="&quot;o&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="req" value="require('$END$')" description="require" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="dcl" value="document.addEventListener('DOMContentLoaded', function () {&#10;    console.log('ready');&#10;});" description="DOMContentLoaded" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>